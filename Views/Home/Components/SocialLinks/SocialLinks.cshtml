@model IList<ViewComponentsApp.Models.SocialIcon>

<div class="col-md-12" style="padding-top:50px;">
    @foreach (var icon in Model)
    {
        <a style="background:@icon.IconBgColor" href="@icon.IconTargetUrl">
            <i class="@icon.IconClass"></i>
            @icon.IconClass
        </a>
    }
</div>     